<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>

	<title><%= name %> | Web Designer, Director | name@yourdomain.com</title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />

	<meta name="keywords" content="" />
	<meta name="description" content="" />

	<link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/2.7.0/build/reset-fonts-grids/reset-fonts-grids.css" media="all" /> 
	<link rel="stylesheet" type="text/css" href="resume.css" media="all" />

</head>
<body>

<div class="yui-t7">
	<div id="inner">
	
		<div id="hd">
			<div class="yui-gc">
				<div class="yui-u first">
					<h1><%= name %></h1>
					<h2><%= title %></h2>
				</div>

				<div class="yui-u">
					<div class="contact-info">
						<h3><%= phone %></h3>
						<h3><%= email %></h3>
						<h3><%= site %></h3>
						<h3><%= linkedin %></h3>
					</div><!--// .contact-info -->
				</div>
			</div><!--// .yui-gc -->
		</div><!--// hd -->

		<div id="bd">
			<div id="yui-main">
				<div class="yui-b">
					<% if(profile.visible) { %>
						<div class="yui-gf">
							<div class="yui-u first">
								<h2>Profile</h2>
							</div>
							<div class="yui-u">
								<p class="enlarge"><%= profile.content %></p>
							</div>
						</div><!--// .yui-gf -->
					<% } %>

					<% if(skills.visible) { %>
						<div class="yui-gf">
							<div class="yui-u first">
								<h2>Main Skills</h2>
							</div>
							<div class="yui-u">
									<% for(var i = 0; i < skills.items.length; i++) { %>
										<div class="talent">
											<h2><%= skills.items[i].title %></h2>
											<p><%= skills.items[i].content %></p>
										</div>
									<% } %>
							</div>
						</div><!--// .yui-gf -->
					<% } %>

					<% if(technical.visible) { %>
						<div class="yui-gf">
							<div class="yui-u first">
								<h2>Technical</h2>
							</div>
							<div class="yui-u">
								<ul class="talent">
									<li>XHTML</li>
									<li>CSS</li>
									<li class="last">Javascript</li>
								</ul>

								<ul class="talent">
									<li>Jquery</li>
									<li>PHP</li>
									<li class="last">CVS / Subversion</li>
								</ul>

								<ul class="talent">
									<li>OS X</li>
									<li>Windows XP/Vista</li>
									<li class="last">Linux</li>
								</ul>
							</div>
						</div><!--// .yui-gf-->
					<% } %>

					<% if(experiences.visible) { %>
						<div class="yui-gf">
		
							<div class="yui-u first">
								<h2>Experience</h2>
							</div><!--// .yui-u -->

							<div class="yui-u">
								<% for(var i = 0; i < experiences.items.length; i++) {
									var experience = experiences.items[i]; %>
									<div class="job">
										<h2><%= experience.company.name %></h2>
										<% for(var i1=0; i1 < experience.occupations.length; i1++) {
											var occupation = experience.occupations[i1]; %>
											<div style="position: relative">
												<h3><%= occupation.title %></h3>
												<h4><%= occupation.begin %></h4>
												<p><%= occupation.tasks %></p>
											</div>
										<% } %>
									</div>
								<% } %>
							</div><!--// .yui-u -->
						</div><!--// .yui-gf -->
					<% } %>

					<div class="yui-gf last">
						<div class="yui-u first">
							<h2>Education</h2>
						</div>
						<div class="yui-u">
							<h2>Indiana University - Bloomington, Indiana</h2>
							<h3>Dual Major, Economics and English &mdash; <strong>4.0 GPA</strong> </h3>
						</div>
					</div><!--// .yui-gf -->


				</div><!--// .yui-b -->
			</div><!--// yui-main -->
		</div><!--// bd -->

	</div><!-- // inner -->


</div><!--// doc -->


</body>
</html>

